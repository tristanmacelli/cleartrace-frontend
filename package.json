    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.16.1",
    "npm-run-all": "^4.1.5",
